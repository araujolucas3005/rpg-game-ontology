@prefix : <http://example.com#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix gufo: <http://purl.org/nemo/gufo#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://example.com#> .

<http://example.com> rdf:type owl:Ontology ;
                      rdfs:comment "RPG Game Design Pattern Ontology." .

#################################################################
#    Object Properties
#################################################################

###  http://example.com#drops
:drops rdf:type owl:ObjectProperty ;
       owl:inverseOf :isWeaponDropOf ;
       rdfs:domain :Drop ;
       rdfs:range :Weapon ;
       rdfs:label "drops"@en .


###  http://example.com#hasBehavior
:hasBehavior rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             owl:inverseOf :isBehaviorOf ;
             rdfs:domain :CommonEnemy ;
             rdfs:range :Behavior .


###  http://example.com#hasBoss
:hasBoss rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         owl:inverseOf :hasQuest .


###  http://example.com#hasClass
:hasClass rdf:type owl:ObjectProperty ;
          owl:inverseOf :isClassOf ;
          rdfs:domain :Character ;
          rdfs:range :Class ;
          rdfs:label "hasClass"@en .


###  http://example.com#hasCombinationIn
:hasCombinationIn rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  owl:inverseOf :isCombinationFrom .


###  http://example.com#hasDrop
:hasDrop rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         owl:inverseOf :isDropOf ;
         rdfs:domain :Enemy ;
         rdfs:range :Drop .


###  http://example.com#hasLocation
:hasLocation rdf:type owl:ObjectProperty ;
             owl:inverseOf :isLocationOf ;
             rdfs:domain :LocatedEntity ;
             rdfs:range :Place ;
             rdfs:label "hasLocation"@en .


###  http://example.com#hasNextMainQuest
:hasNextMainQuest rdf:type owl:ObjectProperty ,
                           owl:TransitiveProperty ;
                  rdfs:domain :MainQuest ;
                  rdfs:range :MainQuest ;
                  rdfs:label "hasNextMainQuest"@en .


###  http://example.com#hasNextSkill
:hasNextSkill rdf:type owl:ObjectProperty ,
                       owl:TransitiveProperty ;
              rdfs:domain :Skill ;
              rdfs:range :Skill ;
              rdfs:label "hasNextSkill"@en .


###  http://example.com#hasQuest
:hasQuest rdf:type owl:ObjectProperty ;
          rdfs:domain :Boss ;
          rdfs:range :Quest ;
          rdfs:label "hasParticipationIn"@en .


###  http://example.com#hasRace
:hasRace rdf:type owl:ObjectProperty ;
         owl:inverseOf :isRaceOf ;
         rdfs:domain :LivingBeing ;
         rdfs:range :Race ;
         rdfs:label "hasRace"@en .


###  http://example.com#hasRarity
:hasRarity rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           owl:inverseOf :isRarityOf ;
           rdfs:domain :Weapon ;
           rdfs:range :Rarity .


###  http://example.com#hasSkill
:hasSkill rdf:type owl:ObjectProperty ;
          owl:inverseOf :isSkillOf ;
          rdfs:domain :Class ;
          rdfs:range :Skill ;
          rdfs:label "hasSkill"@en .


###  http://example.com#hasWeapon
:hasWeapon rdf:type owl:ObjectProperty ;
           owl:inverseOf :isWeaponOf ;
           rdfs:domain :Class ;
           rdfs:range :Weapon ;
           rdfs:label "hasWeapon"@en .


###  http://example.com#isBehaviorOf
:isBehaviorOf rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty .


###  http://example.com#isClassOf
:isClassOf rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty .


###  http://example.com#isCombinationFrom
:isCombinationFrom rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain :Race ;
                   rdfs:range :Race .


###  http://example.com#isDropOf
:isDropOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty .


###  http://example.com#isLocationOf
:isLocationOf rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty .


###  http://example.com#isRaceOf
:isRaceOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty .


###  http://example.com#isRarityOf
:isRarityOf rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty .


###  http://example.com#isSkillOf
:isSkillOf rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty .


###  http://example.com#isWeaponDropOf
:isWeaponDropOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty .


###  http://example.com#isWeaponOf
:isWeaponOf rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://example.com#hasAttackPower
:hasAttackPower rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf owl:topDataProperty ;
                rdfs:domain :AttackWeapon ;
                rdfs:range xsd:decimal .


###  http://example.com#hasBaseAttackPower
:hasBaseAttackPower rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf owl:topDataProperty ;
                    rdfs:domain :LivingBeing ;
                    rdfs:range xsd:decimal .


###  http://example.com#hasBaseDefensePower
:hasBaseDefensePower rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf owl:topDataProperty ;
                     rdfs:domain :LivingBeing ;
                     rdfs:range xsd:decimal .


###  http://example.com#hasBaseHealthPoints
:hasBaseHealthPoints rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf owl:topDataProperty ;
                     rdfs:domain :LivingBeing ;
                     rdfs:range xsd:decimal .


###  http://example.com#hasBaseManaPoints
:hasBaseManaPoints rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf owl:topDataProperty ;
                   rdfs:domain :LivingBeing ;
                   rdfs:range xsd:decimal .


###  http://example.com#hasDefensePower
:hasDefensePower rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdfs:domain :DefenseWeapon ;
                 rdfs:range xsd:decimal .


###  http://example.com#hasDescription
:hasDescription rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf owl:topDataProperty ;
                rdfs:domain :DescribedEntity ;
                rdfs:range xsd:string .


###  http://example.com#hasDropRate
:hasDropRate rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain :Drop ;
             rdfs:range xsd:decimal .


###  http://example.com#hasExpGain
:hasExpGain rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:domain :ExpGiver ;
            rdfs:range xsd:decimal .


###  http://example.com#hasGoldGain
:hasGoldGain rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain :GoldGiver ;
             rdfs:range xsd:decimal .


###  http://example.com#hasName
:hasName rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:domain :DescribedEntity ;
         rdfs:range xsd:string .


###  http://example.com#hasRequiredManaPoints
:hasRequiredManaPoints rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf owl:topDataProperty ;
                       rdfs:domain :Skill ;
                       rdfs:range xsd:decimal .


###  http://example.com#hasSkillPower
:hasSkillPower rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain :Skill ;
               rdfs:range xsd:decimal .


#################################################################
#    Classes
#################################################################

###  http://example.com#AgressiveCommonEnemy
:AgressiveCommonEnemy rdf:type owl:Class ;
                      owl:equivalentClass [ owl:intersectionOf ( :CommonEnemy
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :hasBehavior ;
                                                                   owl:hasValue :Agressive
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] ;
                      owl:disjointWith :PassiveCommonEnemy ;
                      rdfs:comment "Common enemies that are aggresive." .


###  http://example.com#AttackWeapon
:AttackWeapon rdf:type owl:Class ;
              rdfs:subClassOf :Weapon ;
              owl:disjointWith :DefenseWeapon ;
              rdfs:comment "Weapon that is used to inflict damage on enemies." .


###  http://example.com#Behavior
:Behavior rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :isBehaviorOf ;
                            owl:someValuesFrom :Enemy
                          ] ;
          rdfs:comment "Describes common's enemy behavior." .


###  http://example.com#Boss
:Boss rdf:type owl:Class ;
      rdfs:subClassOf :Enemy ;
      owl:disjointWith :CommonEnemy ;
      rdfs:comment "Bosses are enemies that are stronger than the common ones. They usually appears at the end of a Main Quest." .


###  http://example.com#Character
:Character rdf:type owl:Class ;
           rdfs:subClassOf :LivingBeing ;
           owl:disjointWith :Enemy ;
           rdfs:comment "A character is a living being that has the purpose of defeating enemies and explore the world." .


###  http://example.com#Chimera
:Chimera rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( :LivingBeing
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasRace ;
                                                      owl:someValuesFrom :MixedRace
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
         rdfs:comment "Chimeras are living beings that have mixed races." .


###  http://example.com#Class
:Class rdf:type owl:Class ;
       rdfs:subClassOf :DescribedEntity ;
       rdfs:comment "Class of a human. Example of classes: Mage, Warrior, Archer." .


###  http://example.com#CommonEnemy
:CommonEnemy rdf:type owl:Class ;
             rdfs:subClassOf :Enemy ;
             rdfs:comment "Common enemies can be appear at multiples locations of the world." .


###  http://example.com#CommonWeapon
:CommonWeapon rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( :Weapon
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :hasRarity ;
                                                           owl:hasValue :Common
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:comment "Weapons that drops frequently." .


###  http://example.com#DefenseWeapon
:DefenseWeapon rdf:type owl:Class ;
               rdfs:subClassOf :Weapon ;
               rdfs:comment "Weapons that are used to defend from enemies attacks." .


###  http://example.com#DescribedEntity
:DescribedEntity rdf:type owl:Class ;
                 rdfs:comment "Some entity that has description." .


###  http://example.com#Drop
:Drop rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :isDropOf ;
                        owl:someValuesFrom :Enemy
                      ] .


###  http://example.com#Enemy
:Enemy rdf:type owl:Class ;
       rdfs:subClassOf :ExpGiver ,
                       :GoldGiver ,
                       :LivingBeing ,
                       :LocatedEntity ;
       rdfs:comment "Enemies are living beings that tries to stop character's progress." .


###  http://example.com#ExpGiver
:ExpGiver rdf:type owl:Class ;
          rdfs:comment "Some entity that gives experience. For example, when a character slays an enemy, experience points are given to him." .


###  http://example.com#GoldGiver
:GoldGiver rdf:type owl:Class ;
           rdfs:comment "Some entity that gives experience. For example, when a character slays an enemy, gold is given to him." .


###  http://example.com#LivingBeing
:LivingBeing rdf:type owl:Class ;
             rdfs:subClassOf :DescribedEntity ;
             rdfs:comment "All things that lives." .


###  http://example.com#LocatedEntity
:LocatedEntity rdf:type owl:Class ;
               rdfs:comment "Some entity that has a location somewhere." .


###  http://example.com#MainQuest
:MainQuest rdf:type owl:Class ;
           rdfs:subClassOf :Quest ;
           owl:disjointWith :SideQuest ;
           rdfs:comment "A main quest is a mission that has to be accomplished for the story to progress. That said, only the first main quest is not dependent of others." .


###  http://example.com#MainQuestBoss
:MainQuestBoss rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( :Boss
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty :hasQuest ;
                                                            owl:someValuesFrom :MainQuest
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
               rdfs:comment "Bosses that participates in Main Quests." .


###  http://example.com#MixedRace
:MixedRace rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( :Race
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :isCombinationFrom ;
                                                        owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                        owl:onClass :Race
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:comment "Races that are mix from two or more races." .


###  http://example.com#PassiveCommonEnemy
:PassiveCommonEnemy rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( :CommonEnemy
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :hasBehavior ;
                                                                 owl:hasValue :Passive
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:comment "Common enemies that are passive." .


###  http://example.com#Place
:Place rdf:type owl:Class ;
       rdfs:subClassOf :DescribedEntity ;
       rdfs:comment "A location in the world." .


###  http://example.com#Quest
:Quest rdf:type owl:Class ;
       rdfs:subClassOf :DescribedEntity ,
                       :ExpGiver ,
                       :GoldGiver ,
                       :LocatedEntity ;
       rdfs:comment "A mission that a character has to accomplish." .


###  http://example.com#Race
:Race rdf:type owl:Class ;
      rdfs:subClassOf :DescribedEntity ;
      rdfs:comment "Defines an living being. Examples of races: Human, Elf, Dwarf." .


###  http://example.com#RareDroppingWeaponEnemies
:RareDroppingWeaponEnemies rdf:type owl:Class ;
                           owl:equivalentClass [ owl:intersectionOf ( :Enemy
                                                                      [ rdf:type owl:Restriction ;
                                                                        owl:onProperty :hasDrop ;
                                                                        owl:someValuesFrom [ owl:intersectionOf ( :Drop
                                                                                                                  [ rdf:type owl:Restriction ;
                                                                                                                    owl:onProperty :drops ;
                                                                                                                    owl:someValuesFrom [ owl:intersectionOf ( :Weapon
                                                                                                                                                              [ rdf:type owl:Restriction ;
                                                                                                                                                                owl:onProperty :hasRarity ;
                                                                                                                                                                owl:hasValue :Rare
                                                                                                                                                              ]
                                                                                                                                                            ) ;
                                                                                                                                         rdf:type owl:Class
                                                                                                                                       ]
                                                                                                                  ]
                                                                                                                ) ;
                                                                                             rdf:type owl:Class
                                                                                           ]
                                                                      ]
                                                                    ) ;
                                                 rdf:type owl:Class
                                               ] ;
                           rdfs:comment "Enemies that can drop rare weapons when killed." .


###  http://example.com#RareWeapon
:RareWeapon rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( :Weapon
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty :hasRarity ;
                                                         owl:hasValue :Rare
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:comment "Weapons that drops rarely." .


###  http://example.com#Rarity
:Rarity rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :isRarityOf ;
                          owl:someValuesFrom :Weapon
                        ] ;
        rdfs:comment "Enumeration that describes the rarity of an weapon." .


###  http://example.com#SideQuest
:SideQuest rdf:type owl:Class ;
           rdfs:subClassOf :Quest ;
           rdfs:comment "Side quests are missions that can be accomplished anytime and doesn't necessarily have a direct impact on the story." .


###  http://example.com#Skill
:Skill rdf:type owl:Class ;
       rdfs:subClassOf :DescribedEntity ;
       rdfs:comment "Skills are different movesets from a class. For example, a mage can have the Fireball Skill. A warrior can have a Bleed Skill. In the end, is something that is not an normal attack." .


###  http://example.com#SuperRareDroppingWeaponEnemies
:SuperRareDroppingWeaponEnemies rdf:type owl:Class ;
                                owl:equivalentClass [ owl:intersectionOf ( :Enemy
                                                                           [ rdf:type owl:Restriction ;
                                                                             owl:onProperty :hasDrop ;
                                                                             owl:someValuesFrom [ owl:intersectionOf ( :Drop
                                                                                                                       [ rdf:type owl:Restriction ;
                                                                                                                         owl:onProperty :drops ;
                                                                                                                         owl:someValuesFrom [ owl:intersectionOf ( :Weapon
                                                                                                                                                                   [ rdf:type owl:Restriction ;
                                                                                                                                                                     owl:onProperty :hasRarity ;
                                                                                                                                                                     owl:hasValue :SuperRare
                                                                                                                                                                   ]
                                                                                                                                                                 ) ;
                                                                                                                                              rdf:type owl:Class
                                                                                                                                            ]
                                                                                                                       ]
                                                                                                                     ) ;
                                                                                                  rdf:type owl:Class
                                                                                                ]
                                                                           ]
                                                                         ) ;
                                                      rdf:type owl:Class
                                                    ] ;
                                rdfs:comment "Enemies that can drop super rare weapons when killed." .


###  http://example.com#SuperRareWeapon
:SuperRareWeapon rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( :Weapon
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :hasRarity ;
                                                              owl:hasValue :SuperRare
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:comment "Weapons that drops super rarely." .


###  http://example.com#Weapon
:Weapon rdf:type owl:Class ;
        rdfs:subClassOf :DescribedEntity ;
        rdfs:comment "Weapons are used by characters to inflict or defend damage." .


