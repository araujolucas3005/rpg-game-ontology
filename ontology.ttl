#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Class
                :LivingBeing
                :Place
                :Quest
                :Race
                :Skill
                :Weapon
              )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :Animal
                        :Demon
                        :Dwarf
                        :Elf
                        :Human
                        :Undead
